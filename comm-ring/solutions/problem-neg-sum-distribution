(additive-inverse-axiom zero a mina)
(additive-inverse-axiom zero aplusb minaplusb)
(additive-inverse-axiom zero b minb)
(set-equal-by-sum minaplusminb minaplusminb mina minb)
(set-equal-to-self a)
(set-equal-to-self minaplusb)
(swap-sum zero a mina)
(add-zero a a zero)
(refactor-sum a a zero b minb aplusb)
(swap-sum a aplusb minb)
(refactor-sum zero mina a minb aplusb minaplusminb)
(add-zero minaplusb minaplusb zero)
(swap-sum minaplusb minaplusb zero)
(add-zero minaplusminb minaplusminb zero)
(associative-addition-axiom minaplusb minaplusminb zero zero minaplusminb aplusb minaplusb)
