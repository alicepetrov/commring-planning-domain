(additive-inverse-axiom zero a mina)
(additive-inverse-axiom zero aplusminb minaplusminb)
(additive-inverse-axiom zero b minb)
(set-equal-to-self mina)
(set-equal-to-self minaplusminb)
(swap-sum zero aplusminb minaplusminb)
(add-zero mina mina zero)
(refactor-sum mina mina zero b minb minaplusb)
(swap-sum mina minaplusb minb)
(swap-sum minaplusb mina b)
(refactor-sum zero a mina minb minaplusb aplusminb)
(add-zero minaplusminb minaplusminb zero)
(refactor-sum minaplusminb minaplusminb zero aplusminb minaplusb zero)
(associative-addition-axiom minaplusminb minaplusb zero mina b minb minaplusb)
; cost = 14 (unit cost)